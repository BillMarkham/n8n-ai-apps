{
  "name": "n8n-ai-apps",
  "version": "1.0.0",
  "description": "Collection of AI-connected web apps for n8n workflows, with a shared local CORS proxy and per-app UIs.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start:proxy": "cd cors-proxy && nodemon server.js",
    "start:pinecone": "npx live-server apps/chat-pinecone --port=52966",
    "start:sql": "npx live-server apps/chat-sql --port=52967",
    "start:html": "npx live-server apps/chat-html --port=52968",
    "dev": "npm-run-all --parallel start:proxy start:pinecone"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "live-server": "^1.2.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5"
  },
  "author": "Bill Markham",
  "license": "MIT"
}
